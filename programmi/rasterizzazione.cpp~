#include <iostream>
#include <list>
#include <vector>
#include <utility>
#include "rast.cpp"

using namespace std;



int main() {
	
	list<vector<int> > punti = rast3d(1,3,6,10,7,1);

	
	//list<pair<int> > punti = rast(1, 1, 1, 10);
	punti.unique();
	for(list<vector<int> >::iterator it = punti.begin(); it != punti.end(); ++it) {
		vector<int> punto = *it;
		
		cout<<"x:"<<punto[0]<<" y:"<<punto[1]<<" z:"<<punto[2]<<endl;
	}	

	return 0;
}





